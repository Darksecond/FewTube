<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, minimal-ui">

  <title>FEWTUBE</title>
  <script src="/shaka-player.compiled-3.0.10.js"></script>
  <script src="/plyr-3.6.4.js"></script>
  <link rel="stylesheet" href="/plyr-3.6.4.css" />

  <link rel="stylesheet" href="/normalize.css">
  <link rel="stylesheet" href="/milligram.css">
  <link rel="stylesheet" href="/main.css" />
</head>

<body>

  <nav class="navigation">
    <section class="container">
      <a href="/feed">FEWTUBE</a>
    </section>
  </nav>

  <section class="container">

    <hr />
    
    <div class="row">
      <div class="column column-66">
        {{#feed}}
        <div class="list-video">
          <a href="/watch/{{id}}">{{title}}</a> <a href="/channel/{{channel.id}}" class="channel">{{channel.title}}</a>
        </div>
        {{/feed}}
      </div>

      <div class="column column-33">
        <h4>SUBSCRIPTIONS</h4>
        {{#subscriptions}}
        <div class="list-video">
          <a href="/channel/{{id}}">{{title}}</a>
        </div>
        {{/subscriptions}}
      </div>
    </div>
    
  </section>


</body>

</html>